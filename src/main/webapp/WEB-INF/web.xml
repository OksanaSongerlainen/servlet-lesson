<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>World Time Application</display-name>

  <servlet>
    <servlet-name>TimeServlet</servlet-name>
    <servlet-class>servlets.TimeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>TimeServlet</servlet-name>
    <url-pattern>/minsk</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TimeServlet</servlet-name>
    <url-pattern>/washington</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TimeServlet</servlet-name>
    <url-pattern>/beijing-xml</url-pattern>
  </servlet-mapping>

</web-app>

